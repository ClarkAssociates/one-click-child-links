{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "local>Tools/Renovate"
    ],
    "semanticCommits": "enabled",
    "packageRules": [
      {
        "matchUpdateTypes": [
          "minor",
          "patch"
        ],
        "groupName": "Minor and Patch Updates"
      }
    ]
  }